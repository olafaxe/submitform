<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <title>Document</title>
    <style>
      table {
        border: 1px solid black;
        border-collapse: collapse;
      }
      td {
        border: 1px solid black;
      }
    </style>
  </head>
  <body>
    <table>
      <tr>
        <th>Name</th>
        <th>email</th>
      </tr>

      <%output.forEach((e) => {%> <%if(e.user === "") {return;}else{%>
      <tr>
        <td><%=e.user%></td>
        <td><%=e.email%></td>
      </tr>

      <%}});%>
    </table>
  </body>
</html>
